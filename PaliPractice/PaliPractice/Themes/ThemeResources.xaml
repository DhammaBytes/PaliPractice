<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <!-- Light Theme -->
        <ResourceDictionary x:Key="Light">
            <!-- Primary Colors (ochre) -->
            <SolidColorBrush x:Key="PrimaryBrush" Color="#B35C00"/>
            <SolidColorBrush x:Key="PrimaryInverseBrush" Color="#FFD8B5"/>
            <SolidColorBrush x:Key="PrimaryContainerBrush" Color="#FFE2C6"/>
            <SolidColorBrush x:Key="OnPrimaryBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnPrimaryContainerBrush" Color="#3A1E00"/>

            <!-- Secondary Colors (muted ochre-brown) -->
            <SolidColorBrush x:Key="SecondaryBrush" Color="#8A5A1A"/>
            <SolidColorBrush x:Key="SecondaryContainerBrush" Color="#F2D8B5"/>
            <SolidColorBrush x:Key="OnSecondaryBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnSecondaryContainerBrush" Color="#2F1600"/>

            <!-- Tertiary Colors -->
            <SolidColorBrush x:Key="TertiaryBrush" Color="#0061A4"/>
            <SolidColorBrush x:Key="TertiaryContainerBrush" Color="#CFE4FF"/>
            <SolidColorBrush x:Key="OnTertiaryBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnTertiaryContainerBrush" Color="#001D36"/>

            <!-- Error Colors -->
            <SolidColorBrush x:Key="ErrorBrush" Color="#B3261E"/>
            <SolidColorBrush x:Key="ErrorContainerBrush" Color="#F9DEDC"/>
            <SolidColorBrush x:Key="OnErrorBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnErrorContainerBrush" Color="#410E0B"/>

            <!-- Success Colors -->
            <SolidColorBrush x:Key="SuccessBrush" Color="#2E7D32"/>
            <SolidColorBrush x:Key="SuccessContainerBrush" Color="#C8E6C9"/>
            <SolidColorBrush x:Key="OnSuccessBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnSuccessContainerBrush" Color="#1B5E20"/>

            <!-- Background and Surface Colors -->
            <SolidColorBrush x:Key="BackgroundBrush" Color="#FCFBFF"/>
            <SolidColorBrush x:Key="OnBackgroundBrush" Color="#1C1B1F"/>
            <SolidColorBrush x:Key="OnBackgroundMediumBrush" Color="#8B8494"/>
            <SolidColorBrush x:Key="SurfaceBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="SurfaceVariantBrush" Color="#F2EFF5"/>
            <SolidColorBrush x:Key="SurfaceInverseBrush" Color="#313033"/>
            <SolidColorBrush x:Key="SurfaceTintBrush" Color="#B35C00"/>
            <SolidColorBrush x:Key="OnSurfaceBrush" Color="#1C1B1F"/>
            <SolidColorBrush x:Key="OnSurfaceVariantBrush" Color="#8B8494"/>
            <SolidColorBrush x:Key="OnSurfaceInverseBrush" Color="#F4EFF4"/>
            
            <!-- Outline Colors -->
            <SolidColorBrush x:Key="OutlineBrush" Color="#79747E"/>
            <SolidColorBrush x:Key="OutlineVariantBrush" Color="#C9C5D0"/>
        </ResourceDictionary>

        <!-- Dark Theme -->
        <ResourceDictionary x:Key="Dark">
            <!-- Primary Colors (ochre) -->
            <SolidColorBrush x:Key="PrimaryBrush" Color="#FFC27A"/>
            <SolidColorBrush x:Key="PrimaryInverseBrush" Color="#6D2F00"/>
            <SolidColorBrush x:Key="PrimaryContainerBrush" Color="#6D3A00"/>
            <SolidColorBrush x:Key="OnPrimaryBrush" Color="#3A1E00"/>
            <SolidColorBrush x:Key="OnPrimaryContainerBrush" Color="#FFE7CF"/>
            
            <!-- Secondary Colors (muted ochre-brown) -->
            <SolidColorBrush x:Key="SecondaryBrush" Color="#E2C39A"/>
            <SolidColorBrush x:Key="SecondaryContainerBrush" Color="#5A3B14"/>
            <SolidColorBrush x:Key="OnSecondaryBrush" Color="#332D41"/>
            <SolidColorBrush x:Key="OnSecondaryContainerBrush" Color="#FFE2C6"/>
            
            <!-- Tertiary Colors -->
            <SolidColorBrush x:Key="TertiaryBrush" Color="#9FCAFF"/>
            <SolidColorBrush x:Key="TertiaryContainerBrush" Color="#00497D"/>
            <SolidColorBrush x:Key="OnTertiaryBrush" Color="#003258"/>
            <SolidColorBrush x:Key="OnTertiaryContainerBrush" Color="#D1E4FF"/>
            
            <!-- Error Colors -->
            <SolidColorBrush x:Key="ErrorBrush" Color="#FFB4AB"/>
            <SolidColorBrush x:Key="ErrorContainerBrush" Color="#93000A"/>
            <SolidColorBrush x:Key="OnErrorBrush" Color="#690005"/>
            <SolidColorBrush x:Key="OnErrorContainerBrush" Color="#FFDAD6"/>

            <!-- Success Colors -->
            <SolidColorBrush x:Key="SuccessBrush" Color="#81C784"/>
            <SolidColorBrush x:Key="SuccessContainerBrush" Color="#1B5E20"/>
            <SolidColorBrush x:Key="OnSuccessBrush" Color="#1B5E20"/>
            <SolidColorBrush x:Key="OnSuccessContainerBrush" Color="#C8E6C9"/>

            <!-- Background and Surface Colors -->
            <SolidColorBrush x:Key="BackgroundBrush" Color="#1C1B1F"/>
            <SolidColorBrush x:Key="OnBackgroundBrush" Color="#E5E1E6"/>
            <SolidColorBrush x:Key="OnBackgroundMediumBrush" Color="#C9C5D0"/>
            <SolidColorBrush x:Key="SurfaceBrush" Color="#302D37"/>
            <SolidColorBrush x:Key="SurfaceVariantBrush" Color="#47464F"/>
            <SolidColorBrush x:Key="SurfaceInverseBrush" Color="#E6E1E5"/>
            <SolidColorBrush x:Key="SurfaceTintBrush" Color="#FFC27A"/>
            <SolidColorBrush x:Key="OnSurfaceBrush" Color="#E6E1E5"/>
            <SolidColorBrush x:Key="OnSurfaceVariantBrush" Color="#C9C5D0"/>
            <SolidColorBrush x:Key="OnSurfaceInverseBrush" Color="#1C1B1F"/>
            
            <!-- Outline Colors -->
            <SolidColorBrush x:Key="OutlineBrush" Color="#928F99"/>
            <SolidColorBrush x:Key="OutlineVariantBrush" Color="#57545D"/>
        </ResourceDictionary>

        <!-- Default/HighContrast fallback -->
        <ResourceDictionary x:Key="Default">
            <!-- Primary Colors (ochre) -->
            <SolidColorBrush x:Key="PrimaryBrush" Color="#B35C00"/>
            <SolidColorBrush x:Key="PrimaryInverseBrush" Color="#FFD8B5"/>
            <SolidColorBrush x:Key="PrimaryContainerBrush" Color="#FFE2C6"/>
            <SolidColorBrush x:Key="OnPrimaryBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnPrimaryContainerBrush" Color="#3A1E00"/>

            <!-- Secondary Colors (muted ochre-brown) -->
            <SolidColorBrush x:Key="SecondaryBrush" Color="#8A5A1A"/>
            <SolidColorBrush x:Key="SecondaryContainerBrush" Color="#F2D8B5"/>
            <SolidColorBrush x:Key="OnSecondaryBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnSecondaryContainerBrush" Color="#2F1600"/>

            <!-- Tertiary Colors -->
            <SolidColorBrush x:Key="TertiaryBrush" Color="#0061A4"/>
            <SolidColorBrush x:Key="TertiaryContainerBrush" Color="#CFE4FF"/>
            <SolidColorBrush x:Key="OnTertiaryBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnTertiaryContainerBrush" Color="#001D36"/>

            <!-- Error Colors -->
            <SolidColorBrush x:Key="ErrorBrush" Color="#B3261E"/>
            <SolidColorBrush x:Key="ErrorContainerBrush" Color="#F9DEDC"/>
            <SolidColorBrush x:Key="OnErrorBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnErrorContainerBrush" Color="#410E0B"/>

            <!-- Success Colors -->
            <SolidColorBrush x:Key="SuccessBrush" Color="#2E7D32"/>
            <SolidColorBrush x:Key="SuccessContainerBrush" Color="#C8E6C9"/>
            <SolidColorBrush x:Key="OnSuccessBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="OnSuccessContainerBrush" Color="#1B5E20"/>

            <!-- Background and Surface Colors -->
            <SolidColorBrush x:Key="BackgroundBrush" Color="#FCFBFF"/>
            <SolidColorBrush x:Key="OnBackgroundBrush" Color="#1C1B1F"/>
            <SolidColorBrush x:Key="OnBackgroundMediumBrush" Color="#8B8494"/>
            <SolidColorBrush x:Key="SurfaceBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="SurfaceVariantBrush" Color="#F2EFF5"/>
            <SolidColorBrush x:Key="SurfaceInverseBrush" Color="#313033"/>
            <SolidColorBrush x:Key="SurfaceTintBrush" Color="#B35C00"/>
            <SolidColorBrush x:Key="OnSurfaceBrush" Color="#1C1B1F"/>
            <SolidColorBrush x:Key="OnSurfaceVariantBrush" Color="#8B8494"/>
            <SolidColorBrush x:Key="OnSurfaceInverseBrush" Color="#F4EFF4"/>
            
            <!-- Outline Colors -->
            <SolidColorBrush x:Key="OutlineBrush" Color="#79747E"/>
            <SolidColorBrush x:Key="OutlineVariantBrush" Color="#C9C5D0"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>